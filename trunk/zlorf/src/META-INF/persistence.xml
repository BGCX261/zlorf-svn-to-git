<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="zlorfPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>org.zlorf.model.Songs</class>
    <class>org.zlorf.model.Verses</class>
    <class>org.zlorf.model.SongItem</class>
    <class>org.zlorf.model.VlcItem</class>
    <class>org.zlorf.model.OOS</class>
    <class>org.zlorf.model.OOSItem</class>
    <class>org.zlorf.model.VLCItem</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:derby://localhost:1527/zlorf"/>
      <property name="javax.persistence.jdbc.password" value="Philip"/>
      <property name="javax.persistence.jdbc.driver" value="org.apache.derby.jdbc.ClientDriver"/>
      <property name="javax.persistence.jdbc.user" value="mjaggard"/>
    </properties>
  </persistence-unit>
</persistence>
